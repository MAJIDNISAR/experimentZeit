{
    "version": 2,
    "name": "zeitExpressProject",
    "builds": [
        {
            "src": "*.html",
            "use": "@now/static"
        },
        {
            "src": "./bin/www",
            "use": "@now/node"
        },
        {
            "src": "./routes/**/*.js",
            "use": "@now/node"
        },
        {
            "src": "./public/images/**/*",
            "use": "@now/static"
        },
        {
            "src": "./public/javascripts/*.js",
            "use": "@now/node"
        },
        {
            "src": "api/**/*.js",
            "use": "@now/node"
        },
        {
            "src": "*.js",
            "use": "@now/node"
        },
        {
            "src": "./public/stylesheets/*.css",
            "use": "@now/static"
        }
    ],
    "routes": [
        {
            "src": "/",
            "dest": "/routes/index.js"
        }
    ]
}