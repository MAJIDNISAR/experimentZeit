{
    "version": 2,
    "name": "zeitExpressProject",
    "builds": [
        {
            "src": "**/*",
            "use": "@now/static"
        },
        {
            "src": "www/api/**/*.js",
            "use": "@now/node"
        }
    ],
    "routes": [
        {
            "src": "/",
            "dest": "www"
        }
    ]
}